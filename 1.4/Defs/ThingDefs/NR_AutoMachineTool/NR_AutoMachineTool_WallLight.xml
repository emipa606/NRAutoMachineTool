<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>Building_NR_AutoMachineTool_WallLight</defName>
    <label>WallLight</label>
    <thingClass>Building</thingClass>
    <description>Wall light.</description>
    <uiIconPath>NR_AutoMachineTool/Buildings/WallLightIcon</uiIconPath>
    <graphicData>
      <texPath>NR_AutoMachineTool/Buildings/WallLight</texPath>
      <graphicClass>NR_AutoMachineTool.Graphic_WallLight</graphicClass>
      <drawSize>(1, 1)</drawSize>
    </graphicData>
    <building>
      <blueprintGraphicData>
        <texPath>NR_AutoMachineTool/Buildings/WallLight</texPath>
        <graphicClass>NR_AutoMachineTool.Graphic_WallLight</graphicClass>
        <drawSize>(1, 1)</drawSize>
      </blueprintGraphicData>
      <isEdifice>false</isEdifice>
    </building>
    <clearBuildingArea>false</clearBuildingArea>
    <constructEffect>ConstructMetal</constructEffect>
    <costList>
      <Steel>10</Steel>
    </costList>
    <placeWorkers>
      <li>NR_AutoMachineTool.PlaceWorker_WallEmbedded</li>
    </placeWorkers>
    <altitudeLayer>BuildingOnTop</altitudeLayer>
    <fillPercent>0</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>100</WorkToBuild>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>0</Flammability>
    </statBases>
    <size>(1,1)</size>
    <designationCategory>NR_AutoMachineTool_DesignationCategory</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <surfaceType>Item</surfaceType>
    <rotatable>false</rotatable>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>50</basePowerConsumption>
        <alwaysDisplayAsUsingPower>true</alwaysDisplayAsUsingPower>
        <shortCircuitInRain>false</shortCircuitInRain>
      </li>
      <li Class="CompProperties_Flickable" />
      <li Class="CompProperties_Glower">
        <glowRadius>15</glowRadius>
        <glowColor>(217,217,208,0)</glowColor>
      </li>
    </comps>
    <leaveResourcesWhenKilled>True</leaveResourcesWhenKilled>
    <researchPrerequisites>
    </researchPrerequisites>
  </ThingDef>
</Defs>