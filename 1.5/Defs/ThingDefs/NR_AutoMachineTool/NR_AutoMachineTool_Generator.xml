<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>Building_NR_AutoMachineTool_Generator</defName>
    <label>PowerfulPowerPlant</label>
    <thingClass>Building</thingClass>
    <description>Powerful power plant.</description>
    <graphicData>
      <texPath>NR_AutoMachineTool/Buildings/Generator</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(2,2)</drawSize>
    </graphicData>
    <drawerType>MapMeshAndRealTime</drawerType>
    <constructEffect>ConstructMetal</constructEffect>
    <costList>
      <NR_AutoMachineTool_MaterialEnergy>10000</NR_AutoMachineTool_MaterialEnergy>
      <Plasteel>2000</Plasteel>
      <Uranium>2000</Uranium>
      <ComponentSpacer>30</ComponentSpacer>
      <AIPersonaCore>1</AIPersonaCore>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>10000</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(2,2)</size>
    <designationCategory>NR_AutoMachineTool_DesignationCategory</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <hasInteractionCell>false</hasInteractionCell>
    <surfaceType>Item</surfaceType>
    <rotatable>false</rotatable>
    <recipes>
    </recipes>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>-1000000</basePowerConsumption>
        <transmitsPower>true</transmitsPower>
      </li>
    </comps>
    <leaveResourcesWhenKilled>True</leaveResourcesWhenKilled>
    <researchPrerequisites>
      <li>ResearchProject_NR_AutoMachineTool_Generator</li>
    </researchPrerequisites>
    <inspectorTabs>
    </inspectorTabs>
    <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
    <placeWorkers>
    </placeWorkers>
    <modExtensions>
      <li Class="NR_AutoMachineTool.ModExtension_AutoMachineTool">
        <outputCellResolverType>NR_AutoMachineTool.Building_ShieldTargetCellResolver</outputCellResolverType>
      </li>
    </modExtensions>
  </ThingDef>
</Defs>
